{
  "name": "DKershner.com",
  "description": "Derek Kershner's website.",
  "version": "1.16.0",
  "author": "Derek Kershner",
  "private": true,
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:next",
    "postbuild": "next-sitemap",
    "build:next": "next build",
    "build:cleanup": "find ./lib -name '*.js' -delete && find ./lib -name '*.d.ts' -delete && find ./bin -name '*.js' -delete && find ./bin -name '*.d.ts' -delete",
    "deploy": "cdk deploy --all --require-approval never",
    "export": "next export",
    "build-export": "npm run build && npm run export",
    "start": "next start",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src --ext .tsx,.ts,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint ./src --ext .tsx,.ts,.js,.jsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.110",
    "@mui/material": "^5.10.16",
    "gray-matter": "^4.0.3",
    "next": "^13.0.6",
    "next-connect": "^0.13.0",
    "next-sitemap": "^3.1.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-hook-form": "^7.40.0",
    "react-parallax": "^3.5.1",
    "react-query": "^3.39.2",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-amplify-alpha": "^2.53.0-alpha.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "aws-cdk": "^2.53.0",
    "aws-cdk-lib": "^2.53.0",
    "constructs": "^10.1.177",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-mdx": "^2.0.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
